{
    "version": 2,
    "builds": [
    {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb"
        }
    }, 
    {
        "src": "static/*",
        "use": "@vercel/static"    }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "app.py"
    }
    ]
}